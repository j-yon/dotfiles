Host github.com
  IdentityFile ~/.ssh/git-key

Host tinkercliffs
  HostName tinkercliffs1.arc.vt.edu
  User jadeny
  IdentityFile ~/.ssh/vt-key

Host rlogin
  HostName rlogin.cs.vt.edu
  User jadeny
  IdentityFile ~/.ssh/vt-key

Host hive
  HostName login-hive-rh9.pace.gatech.edu
  User jyon7
  ForwardAgent yes
  IdentityFile ~/.ssh/gatech-key

Host wasp
  HostName wasp.chemistry.gatech.edu
  User jadeny
  ForwardAgent yes
  RequestTTY yes
  IdentityFile ~/.ssh/gatech-key
  RemoteForward 2489 127.0.0.1:2489  # Lemonade
  RemoteCommand tmux new -A -s tmuxSSH
  
Host *
  AddKeysToAgent yes
  ForwardX11 yes
  ServerAliveInterval 60
  ServerAliveCountMax 30
